{
  "expo": {
    "name": "Renew Alert",
    "slug": "renew-alert",
    "owner": "beingahmadowais",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "renewalert",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beingahmadowais.renewalert",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "We need camera access to let you upload images if required.",
        "NSPhotoLibraryUsageDescription": "We need permission to select photos from your gallery.",
        "NSPhotoLibraryAddUsageDescription": "We need permission to save images to your photo library.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads and measure ad performance for the Renew Alert app.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "v9t59v4e4.skadnetwork"
          }
        ]
      },
      "icon": "./assets/icons/Ios-logo.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo.png",
        "backgroundColor": "#5C1428"
      },
      "icon": "./assets/icons/andriod.png",
      "package": "com.devahmad41.renewalert",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-fbsdk-next",
        {
          "appID": "917327644166678",
          "clientToken": "757e3c5a6e54b37e670eaa5332f55a27",
          "displayName": "Renew Alert",
          "scheme": "fb917327644166678",
          "advertiserIDCollectionEnabled": true,
          "autoLogAppEventsEnabled": true,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads and measure ad performance for the Renew Alert app."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d28545f3-090a-4de2-9ed9-c29ba3b107b3"
      }
    }
